		message = bundleName==null ? dw.web.Resource.msg(key) : dw.web.Resource.msg(key, bundleName, "");
	}	
	var omsg = {msg:message}; 
</isscript>
<isprint value="${JSON.stringify(omsg)}" encoding="off"/> 